@model IEnumerable<WebApp.Models.Movies.Person>

@{
    ViewData["Title"] = "Actors";
}

<h1>Actors</h1>

<table class="table table-striped">
    <thead>
    <tr>
        <th>Name</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var actor in Model)
    {
        <tr>
            <td>@actor.PersonName</td>
        </tr>
    }
    </tbody>
</table>
